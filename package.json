    "@capacitor/status-bar": "^5.0.0",
    "@ionic/vue": "^7.0.0",
    "@ionic/vue-router": "^7.0.0",
    "@vitejs/plugin-legacy": "^5.4.3",
    "ionicons": "^7.0.0",
    "tailwindcss": "^3.4.17",
